<div class="inner cover">
  <h1 class="cover-heading">Rails News</h1>

  <div class="row">
    <% @news.each do |news_article| %>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card">
            <%= tech_news_parser(news_article) %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
